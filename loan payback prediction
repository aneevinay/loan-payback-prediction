sk = os.path.getsize(path) / (1024 * 1024)  # MB\n        size_in_memory = df.memory_usage(deep=True).sum() / (1024 * 1024)  # MB\n        rows, cols = df.shape\n\n        summary.append({\n            \"Dataset\": name,\n            \"Size on Disk (MB)\": round(size_on_disk, 2),\n            \"Size in Memory (MB)\": round(size_in_memory, 2),\n            \"# of Rows\": rows,\n            \"# of Cols\": cols\n        })\n\n    return pd.DataFrame(summary)","metadata":{"execution":{"iopub.status.busy":"2025-11-20T05:25:24.60469Z","iopub.execute_input":"2025-11-20T05:25:24.604969Z","iopub.status.idle":"2025-11-20T05:25:24.61201Z","shell.execute_reply.started":"2025-11-20T05:25:24.604947Z","shell.execute_reply":"2025-11-20T05:25:24.610664Z"},"papermill":{"duration":0.012719,"end_time":"2025-11-19T07:02:36.11693","exception":false,"start_time":"2025-11-19T07:02:36.104211","status":"completed"},"tags":[],"trusted":true},"outputs":[],"execution_count":15},{"cell_type":"code","source":"datasets = [\n    (\"train\", train, train_path),\n    (\"test\", test, test_path)\n]\n\ndataset_summary(datasets)","metadata":{"execution":{"iopub.status.busy":"2025-11-20T05:25:24.613279Z","iopub.execute_input":"2025-11-20T05:25:24.613599Z","iopub.status.idle":"2025-11-20T05:25:25.588874Z","shell.execute_reply.started":"2025-11-20T05:25:24.613567Z","shell.execute_reply":"2025-11-20T05:25:25.587759Z"},"papermill":{"duration":0.930991,"end_time":"2025-11-19T07:02:37.0518","exception":false,"start_time":"2025-11-19T07:02:36.120809","status":"completed"},"tags":[],"trusted":true},"outputs":[{"execution_count":16,"output_type":"execute_result","data":{"text/plain":"  Dataset  Size on Disk (MB)  Size in Memory (MB)  # of Rows  # of Cols\n0   train              53.39               250.50     593994         13\n1    test              21.95               105.41     254569         12","text/html":"<div>\n<style scoped>\n    .dataframe tbody tr th:only-of-type {\n        vertical-align: middle;\n    